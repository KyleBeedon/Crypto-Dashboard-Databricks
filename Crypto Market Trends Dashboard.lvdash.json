    {
      "name": "ded4c1bc",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "\n",
        "-- BTC change since latest data:\n",
        "SELECT pct_change_1d \n",
        "FROM crypto_lag_features \n",
        "WHERE symbol = 'B BTC-USD' \n",
        "ORDER BY scrape_date DESC LIMIT 1;"
      ]
    },
    {
      "name": "62ffaaac",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "-- Volatility (How turbulent the market has been over the past x hours)\n",
        "SELECT ROUND(AVG(volatility_pct),2) as avg_volatility\n",
        "FROM crypto_lag_features \n",
        "WHERE scrape_date = (SELECT MAX(scrape_date) FROM crypto_lag_features);\n"
      ]
    },
    {
      "name": "fea95c59",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "--------------- Price trend line chart ----------------\n",
        "SELECT scrape_date, symbol, price\n",
        "FROM historical_crypto_trends\n",
        "where symbol = \"B BTC-USD\"\n",
        "--WHERE symbol IN ('B BTC-USD', 'E ETH-USD', 'S SOL-USD')\n",
        "ORDER BY scrape_date ASC;\n",
        "\n"
      ]
    }
