  "datasets": [
    {
      "name": "6de6f8fd",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "-- Bitcoin latest price\n",
        "SELECT current_price \n",
        "FROM crypto_lag_features \n",
        "WHERE symbol = 'B BTC-USD' \n",
        "ORDER BY scrape_date DESC LIMIT 1;"
      ]
    },
